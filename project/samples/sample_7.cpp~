#include <iostream>
#include "Matrix.h"
#include <math.h>

using namespace std;
int main () {
Matrix m ( 10,10);
 i , j = i * j;
int i;
int j;
for(i=0 ; i<=9;i++ )
{
for(j=0 ; j<=9;j++ )
{
cout << m(i,j) ;
cout << "  " ;
}
cout << "\n" ;
}
for(i=0 ; i<=9;i++ )
{
for(j=0 ; j<=9;j++ )
{
m(i,j) ;
 }
}
for(i=0 ; i<=9;i++ )
{
for(j=0 ; j<=9;j++ )
{
cout << m(i,j) ;
cout << "  " ;
}
cout << "\n" ;
}
}

